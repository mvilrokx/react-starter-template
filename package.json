    "csswring": "^4.0.0",
    "eslint": "^1.6.0",
    "eslint-friendly-formatter": "^1.2.2",
    "eslint-loader": "^1.0.0",
    "file-loader": "^0.8.1",
    "html-webpack-plugin": "^2.0.3",
    "image-webpack-loader": "^1.6.1",
    "jquery": "^3.0.0-alpha1",
    "node-sass": "^3.3.3",
    "postcss-loader": "^0.6.0",
    "react": "^0.14.0-rc1",
